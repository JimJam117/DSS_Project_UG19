<%- include('partials/header') %>

    <!-- reviews Section -->
    <div class="section">
        <h1>Reviews:</h1>
 
        <% reviews.forEach(function(review){ %>
            <a href="/review/<%= review.id %>" class="list_item">
                <div class="list_item_details">
                    <h2><%= review.title %></h2>
                    <p><%= review.body %></p>
                </div>
                
            </a>
            <% }); %>
 
    </div>

<%- include('partials/footer') %>