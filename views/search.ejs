<%- include('partials/header') %>


    <!-- search results Section -->
    <div class="section">
        <h1>Results for <em><%= query %></em>:</h1>
        <% if(!results.length){ %>
            <p class="no_results">No Results found.</p>
          <% } %> 
        <ul>
        <% results.forEach(function(result){ %>
            <li class="result">
                <a href="<%= result.url %>" class="">
                        <strong><%- result.type %>:</strong> <span class="result_name"><%- result.name %></span>
                </a>
            </li>
            <% }); %>
        </ul>
    </div>

   

<%- include('partials/footer') %>